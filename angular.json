{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"cli": {
		"packageManager": "npm"
	},
	"newProjectRoot": "projects",
	"projects": {
		"shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/shell",
			"sourceRoot": "projects/shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/shell",
						"index": "projects/shell/src/index.html",
						"main": "projects/shell/src/main.ts",
						"polyfills": [
							"zone.js"
						],
						"tsConfig": "projects/shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/shell/src/favicon.ico",
							"projects/shell/src/assets",
							{
								"glob": "**/*",
								"input": "projects/config",
								"output": "/assets/config"
							}
						],
						"styles": [
							"node_modules/@angular-architects/paper-design/assets/css/bootstrap.css",
							"node_modules/@angular-architects/paper-design/assets/scss/paper-dashboard.scss",
							"projects/shell/src/styles.scss"
						],
						"scripts": [],
						"extraWebpackConfig": "projects/shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "shell:build:production",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "shell:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4500,
						"publicHost": "http://localhost:4500",
						"extraWebpackConfig": "projects/shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "shell:build",
						"extraWebpackConfig": "projects/shell/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/shell/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/shell/src/favicon.ico",
							"projects/shell/src/assets"
						],
						"styles": [
							"projects/shell/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		},
		"dyn-routes": {
			"projectType": "library",
			"root": "projects/libs/dyn-routes",
			"sourceRoot": "projects/libs/dyn-routes/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/libs/dyn-routes/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/libs/dyn-routes/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/libs/dyn-routes/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/libs/dyn-routes/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
		},
		"logic-flight": {
			"projectType": "library",
			"root": "projects/libs/logic-flight",
			"sourceRoot": "projects/libs/logic-flight/src",
			"prefix": "lib",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:ng-packagr",
					"options": {
						"project": "projects/libs/logic-flight/ng-package.json"
					},
					"configurations": {
						"production": {
							"tsConfig": "projects/libs/logic-flight/tsconfig.lib.prod.json"
						},
						"development": {
							"tsConfig": "projects/libs/logic-flight/tsconfig.lib.json"
						}
					},
					"defaultConfiguration": "production"
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"tsConfig": "projects/libs/logic-flight/tsconfig.spec.json",
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						]
					}
				}
			}
		},
		"feature-flight-search": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/custom/feature-flight-search",
			"sourceRoot": "projects/custom/feature-flight-search/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/feature-flight-search",
						"index": "",
						"main": "projects/custom/feature-flight-search/src/main.ts",
						"tsConfig": "projects/custom/feature-flight-search/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"extraWebpackConfig": "projects/custom/feature-flight-search/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "500kb",
									"maximumError": "1mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2kb",
									"maximumError": "4kb"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/custom/feature-flight-search/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "feature-flight-search:build:production",
							"extraWebpackConfig": "projects/custom/feature-flight-search/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "feature-flight-search:build:development"
						}
					},
					"defaultConfiguration": "development",
					"options": {
						"port": 4600,
						"publicHost": "http://localhost:4600",
						"extraWebpackConfig": "projects/custom/feature-flight-search/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "feature-flight-search:build",
						"extraWebpackConfig": "projects/custom/feature-flight-search/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"polyfills": [
							"zone.js",
							"zone.js/testing"
						],
						"tsConfig": "projects/custom/feature-flight-search/tsconfig.spec.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/custom/feature-flight-search/src/favicon.ico",
							"projects/custom/feature-flight-search/src/assets"
						],
						"styles": [
							"projects/custom/feature-flight-search/src/styles.scss"
						],
						"scripts": []
					}
				}
			}
		}
	}
}
